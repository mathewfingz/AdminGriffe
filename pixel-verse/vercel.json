{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run build:client",
  "devCommand": "vite --port $PORT",
  "installCommand": "npm install",
  "outputDirectory": "dist/spa",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}